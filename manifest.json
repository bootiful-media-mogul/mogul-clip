{
  "manifest_version": 3,
  "name": "Mogul Clip",
  "version": "1.0.0",
  "description": "clip content to the Mogul system",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "tabs",
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "http://127.0.0.1:8080/*",
    "http://127.0.0.1:1010/*",
    "https://api.media-mogul.io/*",
    "https://media-mogul.io/*",
    "https://studio.media-mogul.io/*"
  ],
  "action": {
    "default_popup": "client.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}